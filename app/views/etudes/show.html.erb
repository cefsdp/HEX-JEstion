            </div>
        </div>
        <div class="footer">
            <%= image_tag 'HEX_BLANC', style: 'height:5vh; width: auto;'%>
        </div>
    </div>
</div>


<% # ==========================================================================================================================%>
<% # Add Etape %>
<div class="hex_modal2" id="add_etape">
    <div class="modal_content">
        <div class="header">
            <div class="modal-close" id="add_etape">
                <i class="fas fa-times"></i>
            </div>
        </div>
        <div class="main">
            <div class="formulaire">
                <div class="title">
                    <div class="ligne">Modifier</div>
                    <div class="ligne">Les Chargés</div>
                    <div class="ligne">de l'étude ?</div>
                </div>
                <div class="content">
                    <div class="button_line">
                        <%= button_to new_junior_etude_phase_path(@junior, @etude, @new_phase), method: :get do %>
